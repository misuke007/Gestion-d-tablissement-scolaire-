    <div class="content-admin-option">


        <div class="row">

            <div class="text-box col-md-4 ">

                <h2 class="nom_filiere">
                    <%=dataFiliere.nom%>
                </h2>

                <div class="underline_filiere"></div>
                <p class="nbr_etudiant">A propos de l'EC: </p>
                <a href="/admin/ec/liste/<%=FiliereId%>"> <i class="fa fa-list"></i> Retour à la liste</a>

            </div>

            <div class="form-box col-md-8">

                <%if(success!= '') { %>
                    <div class="text-center d-flex justify-content-center align-items-center notif-success"><%=success%></div>
                <% } %>

                <%if(error != '' ) { %>
                    <div class="text-center d-flex justify-content-center align-items-center notif"><%=error%></div>
                <% } %>     

            </div>

        </div>


        <div class="row mt-5">
            
            <div class="box-ec col-md-6">
               <h5> Elément constitutif : <span><%=data.nom%></span> </h5>
               <h5> Unité d'enseignement : <span><%=data.UE.nom%></span> </h5>
             <%if(dataOption.OptionId != 0) { %>
                <h5>Option : <span><%=dataOption.Option.nom%></span></h5>
             <% } %>   

            <%if(data.UE.SemestreId == 1 ){ %>
                <h5> Niveau : <span>L1/S1</span> </h5>
            <% } %>
            
            <%if(data.UE.SemestreId == 2 ){ %>
                <h5> Niveau : <span>L1/S2</span> </h5>
            <% } %>   
            
            <%if(data.UE.SemestreId == 3 ){ %>
