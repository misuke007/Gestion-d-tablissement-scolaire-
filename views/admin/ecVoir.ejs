            <% } %>   
            
            <%if(data.UE.SemestreId == 4 ){ %>
                <h5> Niveau : <span>L2/S4</span> </h5>
            <% } %>  
            
            <%if(data.UE.SemestreId == 5 ){ %>
                <h5> Niveau : <span>L3/S5</span> </h5>
            <% } %> 
            
            <%if(data.UE.SemestreId == 6 ){ %>
                <h5> Niveau : <span>L3/S6</span> </h5>
            <% } %> 
            
            <%if(data.UE.SemestreId == 7 ){ %>
                <h5> Niveau : <span>M1/S7</span> </h5>
            <% } %>   
            
            <%if(data.UE.SemestreId == 8 ){ %>
                <h5> Niveau : <span>M1/S8</span> </h5>
            <% } %>   
            
            <%if(data.UE.SemestreId == 9 ){ %>
                <h5> Niveau : <span>M2/S9</span> </h5>
            <% } %>  
            
            <%if(data.UE.SemestreId == 10 ){ %>
                <h5> Niveau : <span>M2/S10</span> </h5>
            <% } %>
            
            <h5> Volume horaire : <span><%=data.VH%> heures</span> </h5>
             
            <%if(data.UtilisateurId !== null) { %>
               
                <h5> Professeur : <span> <%=data.Utilisateur.nom%> <%=data.Utilisateur.prenom%> <a href="/admin/professeur/enlever/<%=data.id%>/<%=FiliereId%>/<%=SemestreId%>/<%=data.Utilisateur.id%>" class="assignation">Désassigner</a></span></span> </h5>
            <% }else{ %>
                <h5> Professeur : <span>Néant</span> </h5>
            <% } %>   


            </div>
        </div>

    </div>











